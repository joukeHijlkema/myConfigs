;;|--------------------------------------------------------------
;;|Description : $1
;;|NOTE : $2
;;|-
;;|Author : jouke hylkema
;;|date   : `(shell-command-to-string "echo -n $(date +%d-%M-%Y' '%H:%M:%S)")`
;;|--------------------------------------------------------------
(defun ${3:fun} (${4:args})
  "$1"
  ${6:(interactive${7: "${8:P}"})}
  $0)